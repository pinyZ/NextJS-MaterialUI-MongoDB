{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error"
  },
  "overrides": [
    {
      "files": [
        "pages/**/*.stories.js",
        "components/**/*.stories.js"
      ],
      "rules": {
        "import/no-anonymous-default-export": "off"
      }
    }
  ]
}